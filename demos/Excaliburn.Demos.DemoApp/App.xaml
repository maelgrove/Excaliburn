             xmlns:demoApp="clr-namespace:Excaliburn.Demos.DemoApp">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <demoApp:AppBootstrapper x:Key="Bootstrapper" />
